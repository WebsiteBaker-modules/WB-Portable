          <!-- END module_uninstall_select_block -->
        </select>
      </td>
      <td width="110">
        <input type="submit" name="submit" value="{TEXT_UNINSTALL}" style="width: 100px;" />
      </td>
    </tr>
  </table>
  <br />
</form>
<!-- END module_uninstall_block -->

