  <table summary="" cellpadding="2" cellspacing="0" border="0" width="100%">
    <tr>
      <td>
        <select name="file" style="width: 77%;">
        <option value="" selected="selected">{TEXT_PLEASE_SELECT}...</option>
        <!-- BEGIN module_uninstall_select_block -->
